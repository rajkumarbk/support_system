<odoo>
    <data>
        <template id="portal_my_invoices" name="ticket_view">

            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>
                <xpath expr="//div[hasclass('addnewdata')]" position="inside"/>

                <t t-call="portal.portal_searchbar">
                    <button class="addnewdata" style="background-color: #581845; color: white; border: 2px; border-radius: 5px;" onclick="window.location.href = '/ticket_raise_form'">New Request</button>
                    <t t-set="title">Ticket Raise</t>
                </t>
                <!--                <t t-if="not invoices">-->
                <!--                    <p>There are currently no Ticket Raise for your account.</p>-->
                <!--                </t>-->


                <t  t-call="portal.portal_table" >
                    <thead>
                        <tr class="active">
                            <th>Ticket Type</th>
                            <th>Employer Of</th>
                            <th>Priority</th>
                            <th>Message</th>
                            <th>photo</th>
                            <th>Requested Date</th>


                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="records" t-as="data">
                            <tr>
                                <td><t t-out="data['ticket_type']"/></td>
                                 <td><t t-out="data['employee']"/></td>
                                 <td><t t-out="data['priority']"/></td>
                                <td><t t-out="data['message']"/></td>
                                <td><t t-out="data['photo']"/></td>
                                <td><t t-out="data['nepalidatepicker']"/></td>
                                


                            </tr>
                        </t>
                </tbody>
                </t>

